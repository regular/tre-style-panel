{
  "name": "tre-style-panel",
  "version": "1.3.0",
  "description": "Edit syle sheets and watch the effect in realtime",
  "main": "index.js",
  "scripts": {
    "test": "tape test/**"
  },
  "keywords": [
    "tre",
    "ssb"
  ],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "ISC",
  "tre-init": {
    "plugins": {
      "tre-client": true,
      "ssb-revisions": true
    },
    "prototypes": {
      "tre-stylesheets": true
    },
    "importers": {
      "tre-stylesheets": true
    },
    "files": {
      "example.css": {
        "path": "example.css",
        "content": {
          "branch": "%stylesheets",
          "root": "%root"
        }
      },
      "basic.css": {
        "path": "basic.css",
        "content": {
          "branch": "%stylesheets",
          "root": "%root"
        }
      },
      "colors.css": {
        "path": "colors.css",
        "content": {
          "branch": "%stylesheets",
          "root": "%root"
        }
      }
    }
  },
  "devDependencies": {
    "ssb-revisions": "^1.2.1",
    "tre-client": "^2.1.1",
    "tre-file-importer": "^2.1.0",
    "tre-finder": "^2.3.0",
    "tre-prototypes": "^1.6.0",
    "tre-split-pane": "^1.0.1"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "module-styles": "^1.0.0",
    "mutant": "^3.22.1",
    "pull-stream": "^3.6.9",
    "tre-string": "^1.0.1",
    "tre-stylesheets": "^1.1.0",
    "tre-editor-shell": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regular/tre-style-panel.git"
  },
  "bugs": {
    "url": "https://github.com/regular/tre-style-panel/issues"
  },
  "homepage": "https://github.com/regular/tre-style-panel#readme"
}
